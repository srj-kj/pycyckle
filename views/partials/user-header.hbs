        <script>
            function sendData(e){
                const searchResults= document.getElementById('searchResults')
                fetch('search',{
                    method:'post',
                    headers:{'Content-Type':'application/json'},
                    body: JSON.stringify({payload: e.value})
                }).then(res =>res.json()).then(data=>{
                        console.log(data)
                    let payload= data.payload
                    searchResults.innerHTML=''
                    if(payload.length < 1){
                        searchResults.innerHTML='<label >Sorry Nothing Found</label>'
                        return;
                    }
                    console.log(payload)
                    payload.forEach((item, index)=>{
                        if(index > 0)searchResults.innerHTML += '<hr>'
                        searchResults.innerHTML += `<p><a href="/product/${item._id}">${item.productName}</a></p>`
                       // searchResults.innerHTML +=`<a href="/product/${item._id}">${item.productName}</a>`
                        searchResults.innerHTML +=`<img style="width:30px; margin-left:-50px;margin-top:-40px;"  src="/images/${item.img[0]}"></img>`
                    })
                    return;
                })
            }
        </script>